    "Stefan Crain (@stefancrain)",
    "The Gitter Badger (@gitter-badger)",
    "Tom Chen (@tomchentw)"
  ],
  "dependencies": {
    "chalk": "2.4.2",
    "commander": "3.0.1",
    "execa": "2.0.4",
    "file-type": "^16.5.1",
    "fs-extra": "8.1.0",
    "globby": "10.0.1",
    "image-marker": "^0.1.2",
    "pretty-bytes": "5.3.0"
  },
  "devDependencies": {
    "@types/execa": "2.0.0",
    "@types/fs-extra": "8.0.0",
    "@types/globby": "9.1.0",
    "@types/node": "12.7.5",
    "nexe": "2.0.0-rc.34",
    "prettier": "1.18.2",
